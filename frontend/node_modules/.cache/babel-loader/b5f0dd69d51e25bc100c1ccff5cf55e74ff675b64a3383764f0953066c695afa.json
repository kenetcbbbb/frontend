simovich/Desktop/frontend/src/pages/About.jsx"],"sourcesContent":["import React from \"react\";\n\nexport const About = () => (\n  <div style={{ padding: \"40px\", maxWidth: \"900px\", margin: \"0 auto\" }}>\n    <h1 style={{ fontSize: \"28px\", marginBottom: \"20px\" }}>О платформе CodeShop</h1>\n    <p style={{ marginBottom: \"16px\", color: \"#4b5563\" }}>\n      CodeShop — это онлайн‑платформа, где вы можете собирать комплексные образовательные решения под себя.\n    </p>\n    <p style={{ marginBottom: \"16px\", color: \"#4b5563\" }}>\n      Мы верим, что обучение должно быть гибким, а курсы — настраиваемыми. Вместо жестких программ вы получаете\n      конструктор, в котором можно выбирать только те модули, которые действительно нужны.\n    </p>\n\n    <h2 style={{ fontSize: \"22px\", marginTop: \"24px\", marginBottom: \"12px\" }}>\n      Как это работает\n    </h2>\n    <ul style={{ color: \"#4b5563\", lineHeight: 1.6 }}>\n      <li>Выбираете базовую услугу или курс.</li>\n      <li>Добавляете дополнительные модули в удобном конструкторе.</li>\n      <li>Сразу видите итоговую стоимость.</li>\n      <li>Оформляете заказ и получаете подтверждение.</li>\n    </ul>\n  </div>\n);"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE1B,MAAO,MAAM,CAAAC,KAAK,CAAGA,CAAA,gBACnBD,KAAA,QAAKE,KAAK,CAAE,CAAEC,OAAO,CAAE,MAAM,CAAEC,QAAQ,CAAE,OAAO,CAAEC,MAAM,CAAE,QAAS,CAAE,CAAAC,QAAA,eACnER,IAAA,OAAII,KAAK,CAAE,CAAEK,QAAQ,CAAE,MAAM,CAAEC,YAAY,CAAE,MAAO,CAAE,CAAAF,QAAA,CAAC,wEAAoB,CAAI,CAAC,cAChFR,IAAA,MAAGI,KAAK,CAAE,CAAEM,YAAY,CAAE,MAAM,CAAEC,KAAK,CAAE,SAAU,CAAE,CAAAH,QAAA,CAAC,kfAEtD,CAAG,CAAC,cACJR,IAAA,MAAGI,KAAK,CAAE,CAAEM,YAAY,CAAE,MAAM,CAAEC,KAAK,CAAE,SAAU,CAAE,CAAAH,QAAA,CAAC,s9BAGtD,CAAG,CAAC,cAEJR,IAAA,OAAII,KAAK,CAAE,CAAEK,QAAQ,CAAE,MAAM,CAAEG,SAAS,CAAE,MAAM,CAAEF,YAAY,CAAE,MAAO,CAAE,CAAAF,QAAA,CAAC,wFAE1E,CAAI,CAAC,cACLN,KAAA,OAAIE,KAAK,CAAE,CAAEO,KAAK,CAAE,SAAS,CAAEE,UAAU,CAAE,GAAI,CAAE,CAAAL,QAAA,eAC/CR,IAAA,OAAAQ,QAAA,